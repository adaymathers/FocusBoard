{
  "name": "everydoc",
  "version": "1.0.0",
  "description": "Sistema integral de gestión académica para estudiantes universitarios",
  "main": "index.js",
  "scripts": {
    "install-all": "npm run install-backend && npm run install-desktop && npm run install-web",
    "install-backend": "cd backend && npm install",
    "install-desktop": "cd desktop && npm install", 
    "install-web": "cd web && npm install",
    "dev": "concurrently \"npm run dev-backend\" \"npm run dev-desktop\"",
    "dev-backend": "cd backend && npm run dev",
    "dev-desktop": "cd desktop && npm run dev",
    "dev-web": "cd web && npm run dev",
    "build": "npm run build-backend && npm run build-desktop && npm run build-web",
    "build-backend": "cd backend && npm run build",
    "build-desktop": "cd desktop && npm run build",
    "build-web": "cd web && npm run build"
  },
  "keywords": ["academic", "organization", "student", "university", "electron", "react"],
  "author": "EveryDoc Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
